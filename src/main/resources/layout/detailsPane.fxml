      <Hyperlink fx:id="googleUrl" layoutX="70.0" layoutY="210.0" onAction="#onGoogleClick" prefHeight="25.0" prefWidth="132.0" text="Hyperlink" AnchorPane.leftAnchor="70.0" AnchorPane.rightAnchor="18.0" />
      <Hyperlink fx:id="wwwUrl" layoutX="70.0" layoutY="193.0" onAction="#onWwwClick" prefHeight="25.0" prefWidth="132.0" text="Hyperlink" AnchorPane.leftAnchor="70.0" AnchorPane.rightAnchor="18.0" />
